# Cody .env example configuration

# Main AI endpoint (LM Studio, OpenAI-compatible)
GEMMA_ENDPOINT="http://10.0.0.201:1234"

# Ollama endpoint (optional)
OLLAMA_ENDPOINT="http://127.0.0.1:11434"

# Default model name
MODEL_NAME="mistralai/codestral-22b-v0.1"

# Memory and history file locations
MEMORY_FILE="$HOME/.cody_global_memory"
HISTORY_FILE="$HOME/.cody_history"

# Default working directory (optional)
# WORK_DIR="/path/to/your/project"

# Number of self-backups to keep
MAX_BACKUPS=5
